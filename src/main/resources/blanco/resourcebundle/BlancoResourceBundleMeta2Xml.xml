<?xml version="1.0" encoding="UTF-8"?>
<!-- blancoCalcParser define file for blancoCsv -->
<blanco version="0.1.0">
  <target name="blancoCalcParser">
    <blancocalcparser name="resourcebundle">
      <propertyblock name="blancoresourcebundle-common" waitY="1">
        <startstring>リソースバンドル定義・共通</startstring>
        <propertykey name="baseName" waitX="2">
          <value>基底名</value>
        </propertykey>
        <propertykey name="locale" waitX="2">
          <value>ロケール</value>
        </propertykey>
        <propertykey name="packageName" waitX="2">
          <value>パッケージ</value>
        </propertykey>
        <propertykey name="suffix" waitX="2">
          <value>サフィックス</value>
        </propertykey>
        <propertykey name="description" waitX="2">
          <value>説明</value>
        </propertykey>
      </propertyblock>

      <tableblock name="blancoresourcebundle-resourceList" waitY="2" titleheight="3" rowname="resource">
        <startstring>リソースバンドル定義・一覧</startstring>

        <tablecolumn name="no">
          <value>No.</value>
        </tablecolumn>
        <tablecolumn name="resourceKey">
          <value>キー</value>
        </tablecolumn>
        <tablecolumn name="resourceString">
          <value>文字列</value>
        </tablecolumn>
      </tableblock>

    </blancocalcparser>
  </target>
</blanco>
